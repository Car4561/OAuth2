<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logeado"
        android:textSize="50dp"
        android:layout_gravity="center_horizontal"
        />
    <Button
        android:id="@+id/btnVerTodosLosMovimientosBancarios"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ver Todos Los Movimientos"
        />
    <Button
        android:id="@+id/btnVerTodosLosMovimientosBancariosUser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ver Todos Los Movimientos User"

        />
</LinearLayout>